      "module_type": "jwt_config",
      "name": "simple-auth",
      "version": "1.0.0",
      "status": "enabled",
      "description": "Basic JWT authentication",
      "dependencies": [],
      "config": {
        "secret_key": "dev-secret-key-change-in-prod",
        "algorithm": "HS256",
        "expiration_minutes": 30,
        "issuer": "basic-llm-dev",
        "audience": "dev-users",
        "refresh_token_enabled": false
      }
    },
    {
      "module_type": "inference_endpoint",
      "name": "llm-chat",
      "version": "1.0.0",
