      "description": "Basic LLM inference for chat",
      "dependencies": ["simple-auth"],
      "config": {
        "model_name": "gpt-3.5-turbo",
        "model_version": "latest",
        "endpoint_url": "https://api.openai.com/v1/chat/completions",
        "system_prompt": "You are a helpful assistant. Be concise and accurate in your responses.",
        "max_tokens": 150,
        "temperature": 0.7,
        "top_p": 1.0,
        "batch_size": 1
      }
    },
    {
      "module_type": "monitoring",
      "name": "basic-logging",
      "version": "1.0.0",
      "status": "enabled",
      "description": "Simple logging and monitoring",
      "dependencies": [],
      "config": {
        "metrics_enabled": true,
        "logging_level": "DEBUG",
        "tracing_enabled": false,
        "health_check_interval": 60,
        "alerting_enabled": false,
        "dashboard_url": null
      }
